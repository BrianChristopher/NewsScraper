<div>
  {{#each hbsObjectMain}}
  <div class="card m-4">
    <div class="card-body">
      <div class="row">
        <div class="col-sm-4">
          <img src={{this.image}} href={{this.link}} class="img-fluid img-thumbnail">
        </div>
        <div class="col-sm-8">
          <a href={{this.link}}>
            <h4>{{this.title}}</h4>
          </a>
          <p>{{this.summary}}</p>
          <button href="#" class="btn btn-primary favoriteButton" data-id={{this._id}}><i class="fas fa-heart"></i> Add to Favorites</button>
        </div>
      </div>
    </div>
  </div>
  {{/each}}
</div>